<Khelldon>

<!-- ######################################################### -->
<!-- ######################################################### -->
<!-- Khelldon - Increase loot default 0.02 now 0.1 -->
<!-- ######################################################### -->
<!-- ######################################################### -->

<set xpath="/entity_classes/entity_class[@name='zombieTemplateMale']/property[@name='LootDropProb']/@value">0.1</set>


<!-- ######################################################### -->
<!-- ######################################################### -->
<!-- Khelldon - Change Starting Gear -->
<!-- ######################################################### -->
<!-- ######################################################### -->

<set xpath="/entity_classes/entity_class[@name='playerMale']/property[@name='ItemsOnEnterGame.GameModeSurvival']/@value">meleeToolTorch,khelldonStarterCrate,workbench,keystoneBlock</set>
<set xpath="/entity_classes/entity_class[@name='playerMale']/property[@name='ItemsOnEnterGame.GameModeSurvivalSP']/@value">meleeToolTorch,khelldonStarterCrate,workbench,keystoneBlock</set>
<set xpath="/entity_classes/entity_class[@name='playerMale']/property[@name='ItemsOnEnterGame.GameModeSurvivalMP']/@value">meleeToolTorch,khelldonStarterCrate,workbench,keystoneBlock</set>









