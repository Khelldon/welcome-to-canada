<!-- ######################################################### -->
<!-- ######################################################### -->
<!-- Khelldon - Canadian Animals -->
<!-- ######################################################### -->
<!-- ######################################################### -->
<append xpath="/entitygroups" >
<entitygroup name="KheldonsAnimalsCanadaHostile">
		<entity name="invisibleAnimalEnemy" prob="6"/>
		<entity name="khelldonMoose"/>
		<entity name="animalSnake"/>
		<entity name="animalZombieVulture"/>
		<entity name="animalWolf"/>
		<entity name="animalDireWolf" prob="0.25"/>
</entitygroup>
<entitygroup name="KheldonsAnimalsCanadaPassive">
		<entity name="invisibleAnimal" prob="6"/>
		<entity name="khelldonBeaver"/>
		<entity name="animalRabbit"/>
		<entity name="animalChicken"/>
		<entity name="animalStag"/>
		<entity name="animalDoe"/>
		<entity name="animalBoar"/>
</entitygroup>
<entitygroup name="KheldonsAnimalsCanadaCity">
		<entity name="invisibleAnimal" prob="6"/>
		<entity name="animalRabbit"/>
		<entity name="animalChicken"/>
		<entity name="animalBoar"/>
</entitygroup>
<entitygroup name="KheldonsAnimalsBearsCanada">
		<entity name="invisibleAnimalEnemy" prob="6"/>
		<entity name="animalBear" prob="0.25"/>
		<entity name="khelldonAnimalBabyBear"/>
</entitygroup>
</append>


<!-- ######################################################### -->
<!-- ######################################################### -->
<!-- Khelldon - Screamers Bear -->
<!-- ######################################################### -->
<!-- ######################################################### -->

<remove xpath="/entitygroups/entitygroup[@name='ZombieScouts']/entity[@name='zombieScreamer']"/>
<append xpath="/entitygroups/entitygroup[@name='ZombieScouts']" >
		<entity name="khelldonScreamerBear"/>
</append>
<!-- Rewrite Scout Groups -->   
<!-- <remove xpath="/entitygroups/entitygroup[starts-with(@name, 'scoutHordeStageGS')]/entity"/> -->
<append xpath="/entitygroups/entitygroup[starts-with(@name, 'scoutHordeStageGS')]" ><entity name="khelldonScreamerBear"/><entity name="khelldonScreamerBearBaby" prob="0.1"/><entity name="khelldonScreamerBearTeen"/><entity name="khelldonScreamerBearHuge" prob="0.05"/></append>


<!-- ######################################################### -->
<!-- ######################################################### -->
<!-- Khelldon - Add New Zombies to Standard Grouping -->
<!-- ######################################################### -->
<!-- ######################################################### -->
<append xpath="/entitygroups/entitygroup[@name='ZombiesAll']" >
		<entity name="zombieScreamer"/>
		<entity name="khelldonStandingSpider"/>
		<entity name="khelldonLeaperScreamer"/>
</append>






