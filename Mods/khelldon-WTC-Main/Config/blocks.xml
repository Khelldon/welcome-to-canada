	<drop event="Fall" name="resourceSnowBall" count="20" prob="0.187" stick_chance="0"/>
	<property name="Model" value="#@modfolder:Resources/khelldonBlocks.unity3d?khelldonSnowBlock"/>
	<property class="RepairItems"> <property name="resourceSnowBall" value="30"/> </property>
	<property class="UpgradeBlock">
		<property name="ToBlock" value="khelldonIceBlock"/>
		<property name="Item" value="drinkJarBoiledWater"/>
		<property name="ItemCount" value="1"/>
		<property name="UpgradeHitCount" value="4"/>
	</property>
</block>

