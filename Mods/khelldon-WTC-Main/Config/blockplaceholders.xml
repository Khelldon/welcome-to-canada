		<block name="treeBurntMaple03" prob="1.0" biome="burnt_forest" randomrotation="true"/>
		<block name="treeWinterPine28m" prob="1.0" biome="burnt_forest" randomrotation="true"/>
	</placeholder>
</append>
	
	
	
	
	
	
	
	
	
	
	

